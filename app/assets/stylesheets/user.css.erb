#header{

}
#header_left, #header_center, #header_right{
  display: inline-block;;
}
#header_right{
  position: absolute;
  right: 0;
  margin-right: 4%;
  font-family: 'Waiting for the Sunrise', cursive;

}
#header_right input{
  position: absolute;
  right: 0;
  margin-right: 4%;
  border-radius: 5px;
  background-color:#66B9BF;
  height: 25px;
  width: 70px;
}

#header_center{
margin-left: 14%;
}
#header_left{
  margin-left: 4%;
}
#header_left p{
  margin-left: 4%;
  font-family: 'Waiting for the Sunrise', cursive;
  width: 300px;
}
#header_center img{
    vertical-align: middle;
    margin-top: -14px;
    margin-bottom: 18px;
}
#header_left h1{
  color: #66B9BF;
  margin-bottom: -5px;
  font-family: 'Waiting for the Sunrise', cursive;
}
#navBar{
  background-color: #66B9BF;
  height: 52px;
  font-family: 'Carme', sans-serif;
}
#navBar a{
  font-size: 40px;
  text-decoration: none;
  color: white;
  margin-left: 14.5%;
  margin-bottom: -10px;
}
